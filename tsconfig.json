{
  "compilerOptions": {
    "noImplicitAny": false,
    "preserveSymlinks": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "declaration": true,
    "strict": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "resolveJsonModule": true,
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@root/*": ["/*"],
      "@docs/*": ["documents/*"],
      "@dumiTheme/*": [".dumi/theme/*"]
    }
  },
  "exclude": ["**/node_modules", "**/*.js"]
}
